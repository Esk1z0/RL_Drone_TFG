{
  "env": {
    "world_path": "./simulation_package/worlds/bioloid_env.wbt",
    "reward_json_path": "./test_bioloid.json",
    "no_render": true
  },
  "model": {
    "algorithm": "SAC",
    "policy": "MultiInputPolicy",
    "params": {
      "learning_rate": 0.001,
      "buffer_size": 30,
      "batch_size": 30,
      "tau": 0.005,
      "gamma": 0.99,
      "train_freq": 1,
      "gradient_steps": 1,
      "learning_starts": 10,
      "stats_window_size": 10
    }
  },
  "training": {
    "timesteps": 30,
    "num_envs": 1
  },
  "callback": {
    "n_steps": 10,
    "save_timestamp_every_n_steps": 20,
    "verbose": 1
  }
}